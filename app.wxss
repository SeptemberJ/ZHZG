.container {
    font-family:"Microsoft Yahei", Helvetica Neue, Helvetica, Arial, sans-serif;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
    padding: 20rpx;
    margin-bottom: 80rpx;
}
.containerWrap {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
    padding: 20rpx;
}

page {
    font-family:"Microsoft Yahei", Helvetica Neue, Helvetica, Arial, sans-serif;
    background: #fff;
    height: 100%;
    font-size: 30rpx;
}
.blank{
  width: 100%;
  height: 40rpx;
  background: #f5f5f5;
}
swiper{
  width: 100%;
}
map{
  margin-top: 10rpx;
}
.noData{
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
  margin: 20rpx;
}
.noData image{
	width: 100rpx;
	height: 100rpx;
	margin-bottom: 20rpx;
}
.noData text{
	color: #777;
}
text{
    font-size: 30rpx;
}
