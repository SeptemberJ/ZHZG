page{
  background-image: url(http://gaopin-preview.bj.bcebos.com/133100642933.jpg@!420);
  background-size: cover;
}
/*tab*/
.topTab{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.topTab text{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
.leftRadius{
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  background: #fff;
}
.rightRadius{
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  background: #fff;
}
.active{
  background: #1296db;
  color: #fff;
}
/*时间选择*/
.meetingOrder{
  width: 700rpx;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: #fff;
  position: relative;
  top: 40%;
  left: 50%;
  transform:translate(-50%,-50%);
}
.meetingOrder_tit{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  border-bottom: 1px solid #f5f5f5;
}
.meetingOrder_tit image{
  width: 80rpx;
  height: 80rpx;
}
.meetingOrder_date{
  width: 100%;
  height: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.date_column{
  width: 100%;
  height: 125rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.date_column text{
  width: 100%;
  height: 125rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.column_item{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
picker{
  margin-right: 10px;
}
.meetingOrder_bot{
  width: 100%;
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.orderBt{
  width: 90%;
  height: 100rpx;
  background: #f60;
  color: #fff;
}